<?xml version="1.0" encoding="UTF-8"?>
<Server version="8">
	<Name>OvenMediaEngine</Name>
	<Type>origin</Type>
	<IP>*</IP>

	<Bind>
		<Providers>
			<RTMP>
				<Port>1935</Port>
			</RTMP>
			<WebRTC>
				<Signalling>
					<Port>3333</Port>
				</Signalling>
			</WebRTC>
		</Providers>

		<Publishers>
			<WebRTC>
				<Timeout>30000</Timeout>
			</WebRTC>
		</Publishers>
	</Bind>

	<VirtualHosts>
		<VirtualHost>
			<Name>default</Name>
			
			<Host>
				<Names>
					<Name>*</Name>
				</Names>
			</Host>

			<Applications>
				<Application>
					<Name>app</Name>
					<Type>live</Type>
					<OutputProfiles>
						<OutputProfile>
							<Name>bypass</Name>
							<OutputStreamName>${OriginStreamName}</OutputStreamName>
							<Encodes>
								<Audio>
									<Bypass>true</Bypass>
								</Audio>
								<Video>
									<Bypass>true</Bypass>
								</Video>
							</Encodes>
						</OutputProfile>
					</OutputProfiles>
					<Providers>
						<RTMP />
						<WebRTC />
					</Providers>
					<Publishers>
						<WebRTC />
					</Publishers>
				</Application>
			</Applications>
		</VirtualHost>
	</VirtualHosts>
</Server>
